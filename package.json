{
  "name": "holodex",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "build": "vite build --mode prod",
    "changelog": "changelog-to-html --outputDirectoryPath docs && generate-md --layout ./resources/roryg-ghostwriter --input ./CHANGELOG.md --output ./docs",
